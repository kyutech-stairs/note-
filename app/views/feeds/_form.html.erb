<% if current_user %>
  <% if current_user.liking?(article) %>
    <%= link_to article_feed_path(current_user.feeds.find_by(article_id: article.id), article_id: article.id), method: :delete, remote: true do %>
      <i class="material-icons medium like_cancel">thumb_up_alt</i>
    <%end%>
  <% else %>
    <%= link_to article_feeds_path(article, params: { feed: "like" }), method: :post, remote: true  do%>
      <i class="material-icons medium">thumb_up_alt</i>
    <% end %>
  <% end %>
  <% if current_user.bads?(article) %>
    <%= link_to article_feed_path(current_user.feeds.find_by(article_id: article.id), article_id: article.id), method: :delete, remote: true do %>
      <i class="material-icons medium bad_cancel">thumb_down_alt</i>
    <% end %>
  <% else %>
    <%= link_to article_feeds_path(article, params: { feed: "bad" }), method: :post, remote: true do %>
      <i class="material-icons medium">thumb_down_alt</i>
    <% end %>
  <% end %>
<% end %>
