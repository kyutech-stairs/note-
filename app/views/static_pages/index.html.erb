<div class="container">
  <div class="main_wrapper">
    <div class="row">
      <ul style="list-style: none;">
        <% if @articles.any? %>
          <%= render @articles %>
        <% end %>
      </ul>
    </div>
    <div class="paginate">
      <%= will_paginate @articles %>
    </div>
  </div>
</div>
