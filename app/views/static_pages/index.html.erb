<div class="container">
  <div class="main_wrapper">
    <div class="row">
        <% if @articles.any? %>
          <%= render @articles %>
        <% end %>
    </div>
    <div class="paginate">
      <%= will_paginate @articles, list_classes: %w(pagination justify-content-center) %>
    </div>
  </div>
</div>
